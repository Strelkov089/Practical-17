class Order
    {
        public int Id;
        public decimal Total;
    }
    internal class Program
    {
        static void Main(string[] args)
        {
            var order1 = new Order { Id = 1000, Total= 1500.00m};
            var order2 = order1;
            order2.Total = 2000.00m;
            Console.WriteLine(order1.Total);
        }
    }
