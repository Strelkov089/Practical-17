struct Weather
    {
        public string City;
        public double? Temp;
        public override string ToString()
        {
            if (Temp == null) { return $"{City} нет данных "; }
            else { return $"{City} {Temp} "; }
        }
    }
    internal class Program
    {
        static void Main(string[] args)
        {
            var w1 = new Weather { City = "Moscow", Temp = 18.4 };
            var w2 = new Weather { City = "Sochi", Temp = null };
            Console.WriteLine(w1);
            Console.WriteLine(w2);
            Console.WriteLine(w2.Temp ?? -999);
        }
    }
